{
  "name": "storify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "server": "node server/index.js",
    "server:dev": "cd server && npm run dev",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "db:setup": "npx prisma generate && npx prisma db push",
    "setup": "npm install && (cd server && npm install) && npm run db:setup",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:db:migrate": "docker-compose exec app npx prisma migrate dev",
    "docker:db:generate": "docker-compose exec app npx prisma generate",
    "docker:db:studio": "docker-compose exec app npx prisma studio",
    "docker:shell": "docker-compose exec app sh",
    "docker:setup": "npm run docker:build && npm run docker:up && sleep 5 && npm run docker:db:migrate",
    "vercel:deploy": "vercel --prod",
    "vercel:dev": "vercel dev",
    "vercel:link": "vercel link",
    "vercel:env": "vercel env ls"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.7.0",
    "@tailwindcss/typography": "^0.5.10",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "14.0.3",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.104.0",
    "prisma": "^5.7.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "^2.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
